<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="book">
<insert id="insertBook" useGeneratedKeys="true" keyProperty="BOOK_NO" keyColumn="BOOK_NO">
    INSERT INTO BOOK(BOOK_NO, BOOK_NAME, BOOK_QUANTITY,BOOK_PRICE,BOOK_AUTHOR,BOOK_PUBLISHER)
    VALUES (BOOK_NO.NEXTVAL, #{book_name},#{book_quantitu},#{book_price},#{book_author},#{book_publisher})

</insert>
</mapper>

